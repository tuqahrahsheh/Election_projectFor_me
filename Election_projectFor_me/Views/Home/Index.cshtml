<head>
    <link rel="stylesheet"
          href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.4.0/css/all.min.css" />
    <link href="~/assets/map.css" rel="stylesheet" />
    
    <style>

        .province:hover .mark {
            display: inline;
        }

        .mark {
            display: none;
            color: red;
            transition: color 0.3s ease;
        }

        .province .name {
            display: none;
        }

        .province.active .name {
            display: inline;
        }

        .province:hover .mark {
            display: inline;
        }

        .popup {
            display: none;
            position: absolute;
            background: white;
            border: 1px solid #ccc;
            padding: 15px;
            box-shadow: 0px 0px 10px rgba(0, 0, 0, 0.1);
            z-index: 1000;
        }

            .popup.show {
                display: block;
                position: absolute !important;
                top: 400rem !important;
            }


        .card {
            border: none;
            border-radius: 10px;
            background-color: #ffffff;
            overflow: hidden;
            padding: 15px;
            box-shadow: 0 0 30px rgba(0, 0, 0, 0.1);
        }

        .card-body {
            padding: 15px;
        }

        .card-title {
            font-size: 1.25rem;
            font-weight: bold;
            margin-bottom: 15px;
        }

        .ad-content img, .ad-content video, .ad-content iframe {
            max-width: 100%;
            height: 400PX;
        }

        .card-img, .card-img-top {
            height: 400px;
            border-top-left-radius: var(--bs-card-inner-border-radius);
            border-top-right-radius: var(--bs-card-inner-border-radius);
        }

        .mx-auto {
            margin-right: auto !important;
            margin-left: auto !important;
            height: 600px;
        }

        .container h2 {
            margin-bottom: 30px;
        }

        .carousel-item {
            transition: transform 0.2s;
        }

        .carousel-control-prev-icon, .carousel-control-next-icon {
            color: black;
            background-color: #a1a1a1;
        }

        .card-text {
            direction: rtl;
            text-align: center;
        }

        #adCarousel {
            padding-bottom: 2.5%;
        }

        .custom-container {
            max-width: 900px;
            margin: 0 auto;
            padding: 2rem;
        }

        .custom-card {
            border-radius: 10px;
            overflow: hidden;
            box-shadow: 0 4px 8px rgba(0, 0, 0, 0.1);
            background-color: #fff;
            margin-top: 2rem;
        }

        .custom-card-header {
            background: linear-gradient(to right, #007bff, #0056b3);
            color: #fff;
            text-align: center;
            padding: 1rem;
            font-size: 1.5rem;
        }

        .custom-card-title {
            margin: 0;
            font-weight: bold;
        }

        .custom-card-body {
            padding: 2rem;
            text-align: center;
        }

        .flip-counter {
            display: flex;
            justify-content: center;
            align-items: center;
            gap: 1rem;
        }

        .flip-card {
            width: 60px;
            height: 90px;
            background: #fff;
            border: 2px solid #ddd;
            border-radius: 5px;
            display: flex;
            justify-content: center;
            align-items: center;
            font-size: 2rem;
            color: #333;
            position: relative;
            overflow: hidden;
        }

            .flip-card .flip-card-inner {
                perspective: 1000px;
                position: relative;
                width: 100%;
                height: 100%;
            }

            .flip-card .flip-card-front,
            .flip-card .flip-card-back {
                width: 100%;
                height: 100%;
                background: #fff;
                display: flex;
                justify-content: center;
                align-items: center;
                position: absolute;
                backface-visibility: hidden;
                transition: transform 0.6s;
            }

            .flip-card .flip-card-back {
                transform: rotateX(180deg);
            }

            .flip-card.flip .flip-card-front {
                transform: rotateX(180deg);
            }

            .flip-card.flip .flip-card-back {
                transform: rotateX(0deg);
            }

        .flip-card-wrapper {
            position: relative;
            height: 90px;
        }

        .custom-alert {
            background-color: #ffc107;
            color: #212529;
            padding: 1rem;
            border-radius: 0.5rem;
            font-size: 1.125rem;
            display: inline-block;
            margin-top: 1rem;
        }
        .marquee-container {
   width: 100%;
   background-color: #f8f9fa;
   padding: 15px 0;
 }

 .marquee-content {
   display: flex;
   animation: marquee 20s linear infinite;
 }

 .marquee-item {
   flex: 0 0 auto;
   padding: 0 20px;
   font-size: 1.2rem;
   color: white;
 }

 @@keyframes marquee {
   0% {
     transform: translateX(100%);
   }

   100% {
     transform: translateX(-100%);
   }
 }

 /* Optional: Pause animation on hover */
 .marquee-container:hover .marquee-content {
   animation-play-state: paused;
 }
 .province:hover .mark {
    display: inline;
}

.mark {
    display: none;
    color: red;
    background-color: inherit;
    transition: color 0.3s ease;
    background-color: none;
}

.province.active .name {
    display: none;
}

