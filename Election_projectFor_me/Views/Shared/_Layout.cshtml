    <style>

        .province:hover .mark {
            display: inline;
        }

        .mark {
            display: none;
            color: red;
            transition: color 0.3s ease;
        }

        .province .name {
            display: none;
        }

        .province.active .name {
            display: inline;
        }

        .province:hover .mark {
            display: inline;
        }

        .popup {
            display: none;
            position: absolute;
            background: white;
            border: 1px solid #ccc;
            padding: 15px;
            box-shadow: 0px 0px 10px rgba(0, 0, 0, 0.1);
            z-index: 1000;
        }

            .popup.show {
                display: block;
            }
